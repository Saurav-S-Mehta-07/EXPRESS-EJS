<%- include('includes/header') %>
<body>
    
    <h1 id="heading">Quora Posts</h1>
    <a href="/addpost">Add new Post</a>
    <div class="posts">
        <%for(let post of posts){%>
            <div class="post">
                <h2><%=post.username%></h2>
                <p><%=post.description%></p>
                <form action="/editpost"><button>edit</button></form>
                <br>
                <form action="/deletepost"><button>delete</button></form>
            </div>  
        <%}%>
    </div>
</body>
</html>